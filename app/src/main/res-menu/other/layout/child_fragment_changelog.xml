<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="changelog"
            type="com.yoesuv.infomadiun.menu.other.viewmodels.ChildFragmentChangelogViewModel"/>
    </data>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewChangelog"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:paddingBottom="@dimen/fragment_info_margin"
        android:paddingTop="@dimen/fragment_info_margin"
        tools:listitem="@layout/item_changelog"/>

</layout>